        <node name="active_scene_recognition_ism" pkg="active_scene_recognition_ism" type="active_scene_recognition_ism" output="screen">
                <rosparam file="$(find active_scene_recognition_ism)/launch/settings.yaml"/>
        </node>

	<!--node pkg="rviz" name="rviz" type="rviz" args="/search_visualization.vcg"/-->
</launch>
